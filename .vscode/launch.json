    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) Iniciar",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/app.out",
        "args": ["-f", "./instancias/quality/instances/berlin52FSTCII_q2_g4_p40_r20_s20_rs15.pop", "-C", "0.001", "-K", "2", "-T", "50", "-maxNotImproviment", "100", "-maxIterTabu", "500", "-seed", "13"],
        "stopAtEntry": true,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Habilitar a reformatação automática para gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Definir Tipo de Desmontagem como Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }
    ]
